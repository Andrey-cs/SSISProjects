   <string notr="true">/* Styles for the titleFrame element */
#titleFrame {
  background-color: transparent;
}

/* TEXT SIZE AND COLOR */
QLabel {
    font-size: 22px;
    font-weight: bold;
    letter-spacing: 2px;
}


/* FORM FRAME */
#Form { 
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, 
                                stop:0 #800000, /* Dark Maroon */
                                stop:1 #A52A2A); /* Brownish Maroon */
    color: black;
}


/* INFO FRAME BG COLOR */
#infoFrame {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, 
                                stop:0 #FFD54F, stop:1 #FFB300);
    border-radius: 10px;
}


QLabel {
    color: #800000;  /* Yellow Text */
    font-weight: bold;
}

QLineEdit {
    background-color: white;
    color: black;
    border: 2px solid #800000;  /* Yellow Border */
    padding: 3px;
    border-radius: 5px;
}


/* INPUT FIELDS HEIGHT */
QLineEdit, QComboBox {
    height: 25px;
    border-radius: 5px;
    padding-left: 5px;
}


/* Styles for the functionFrame and its elements */
#functionFrame {
  background-color: transparent;
  border: none;
  border-radius: 5px;
}

/* FOR PUSH BUTTONS */

/* Buttons with Yellow Background and White Text */
QPushButton {
     background-color: #FFC107; /* Yellow */
    color: #800000;
    border-radius: 5px;
    padding: 5px;
    font-weight: bold;
}

/* Hover effect for buttons (Darker Yellow) */
QPushButton:hover {
    background-color: #FFA000; /* Darker Amber Yellow */
}

QPushButton {
    border-radius: 10px;
    padding: 5px;
}

QPushButton {
    font-size: 14px;
    font-weight: bold;
}

QComboBox {
  color: white;
}

QComboBox {
    border: none;
    color: #800000; /* Change text color if needed */
}



/* TABLE */
QTableWidget QHeaderView::section {
    background-color: #FFC107;  /* Goldish Yellow */
    color: #800000;             /* Maroon Text */
    font-weight: bold;
    font-size: 14px;
    padding: 6px;
    border: 1px solid #800000;  /* Maroon Border */
}

QTableWidget {
    background-color: white;       /* Clean base */
    alternate-background-color: #FFEBB5;  /* Soft pastel yellow */
    gridline-color: #800000;        /* Maroon grid */
    border-radius: 5px;
}

QTableWidget::item {
    color: black;              /* Text should be readable */
    font-size: 13px;
    padding: 4px;
}

QTableWidget::item {
    background: #FFD54F; /* Same as header */
    color: black;
}


QTableWidget::item:selected {
    background-color: #800000; /* Deep red (highlights selected row) */
    color: white;              /* White text for readability */
}

QTableWidget {
    background-color: white; /* Match your form's background */
    gridline-color: #dark-yellow;
    alternate-background-color: #yellow;
}
QTableWidget::item {
    background-color: #yellow;
}
QTableWidget::item:alternate {
    background-color: #dark-yellow;
}
